import java.util.Scanner;

public class EvenFibonacciSum {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);


        System.out.print("Enter the value of N: ");
        int n = sc.nextInt();

        int fib1 = 0, fib2 = 1;
        

        System.out.print("0 ");
        System.out.print("1 ");

        
        for (int i = 0; i <=n; i++) {
            int nextFib = fib1 + fib2;
            fib1 = fib2;
            fib2 = nextFib;
            System.out.print(fib2+" ");
        }


    
    }
}
