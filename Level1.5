import java.util.Scanner;
public class reversed {
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("Enter number:");
        int n=s.nextInt();
        int temp=n;
        int rev=0;
        while(temp!=0)
        {
            int digit=temp%10;
            
            rev=rev*10+digit;
            temp/=10;

        }
        System.out.print( rev);
        s.close();
    }
    
}
