import java.util.Scanner;
public class Pascal {
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
       
        int coef=1;
        System.out.print("Enter no.of rows:");
        int n=s.nextInt();
        for(int i=1;i<=n;i++)
        {
            for(int j=1;j<=n-i;j++)
            {
                System.out.print("  ");
            }
            for(int k=1;k<=i;k++)
            {
                if(i==0 || k==0)
                {
                    coef=1;
                }
                else{
                    coef=coef*(i-k+1)/k;
                }
                System.out.print(coef+"   ");
            }
            System.out.println();
            s.close();
        }
    }
    
}
